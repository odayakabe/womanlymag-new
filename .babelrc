{
  "plugins": [
    "transform-class-properties",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "images": "./src/images",
          "layouts": "./src/layouts",
          "styles": "./src/styles"
        }
      }
    ]
  ],
   "env": {
    "test": {
      "presets": [["es2015"], "react"],
      "plugins": ["transform-class-properties"]
    }
  },
}
