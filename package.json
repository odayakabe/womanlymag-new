    "gatsby-plugin-google-analytics": "^1.0.14",
    "gatsby-plugin-netlify": "^1.0.12",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-source-contentful": "^1.3.31",
    "gatsby-transformer-remark": "^1.7.26"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2"
  }
