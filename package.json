{
  "name": "womanly-mag",
  "description": "Womanly Mag",
  "version": "0.0.1",
  "author": "Rae Farine <rfarine1027@gmail.com>",
  "keywords": [
    "gatsby",
    "womanly",
    "womanlymag",
    "women",
    "nonbinary",
    "art",
    "health"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
